CONFIGS=configs

override CFLAGS += -Wall -Wextra -DVERSION='"$(VERSION)"' -O2 -std=gnu99 -I.
override CFLAGS := $(filter-out -Werror,$(CFLAGS))

APPARMOR_PARSER=/sbin/apparmor_parser

LIB_APPARMOR := -lapparmor
KERNEL=$(shell uname -s)
ifeq (,$(findstring $(KERNEL),Darwin DragonFly FreeBSD NetBSD OpenBSD))
LIB_BSD := -lbsd
endif
LIB_Z := -lz
LIB_CRYPT := -lcrypt
LIB_RT := -lrt
LIB_PTHREAD := -lpthread
LIB_AIO := -laio
LIB_SCTP := -lsctp
LIB_DL := -ldl
LIB_IPSEC_MB := -lIPSec_MB
LIB_JPEG := -ljpeg
LIB_JUDY := -lJudy
LIB_KMOD := -lkmod
ifneq ($(findstring musl-gcc,$(CC)),musl-gcc)
LIB_ATOMIC := -latomic
endif
LIB_MATH := -lm
LIB_XXHASH := -lxxhash
LIB_EGL := -lEGL
LIB_GLES2 := -lGLESv2
LIB_GBM := -lgbm
LIB_MD := -lmd
LIB_MPFR := -lmpfr
LIB_ACL := -lacl
LIB_GMP := -lgmp
LIB_C := -lc
LIB_NETWORK := -lnetwork
LIB_SOCKET := -lsocket
LIB_NSL := -lnsl

LIB_ORDER := $(LIB_ACL) $(LIB_AIO) $(LIB_APPARMOR) $(LIB_ATOMIC) $(LIB_BSD) \
	$(LIB_CRYPT) $(LIB_DL) $(LIB_IPSEC_MB) $(LIB_JPEG) $(LIB_JUDY) \
	$(LIB_KMOD) $(LIB_EGL) $(LIB_GLES2) $(LIB_GMP) $(LIB_GBM) $(LIB_MD) \
	$(LIB_MPFR) $(LIB_SCTP) $(LIB_XXHASH) $(LIB_Z) $(LIB_RT) \
	$(LIB_PTHREAD) $(LIB_MATH) $(LIB_NETWORK) $(LIB_SOCKET) $(LIB_NSL) $(LIB_C)

ifeq ($(shell $(CC) -v 2>&1 | grep 'gcc version' | grep -v 'icc' | wc -l),1)
ifeq ($(shell $(CC) -v 2>&1 | grep 'musl-gcc' | wc -l),0)
USING_GCC := yes
endif
endif
ifeq ($(shell $(CXX) -v 2>&1 | grep 'gcc version' | grep -v 'icc' | wc -l),1)
USING_GXX := yes
endif

DIR=test

.SUFFIXES: .c .o

override LDFLAGS += $(CONFIG_LDFLAGS)

#
# Static flags, only to be used when using GCC
#
ifeq ($(STATIC),1)
	override LDFLAGS += -static
	override CFLAGS += -DBUILD_STATIC
endif

ifeq ($(shell uname -s),SunOS)
	CONFIG_LDFLAGS += $(LIB_SOCKET) $(LIB_NSL)
endif
#
#  This must always come after -pthread so that static linking works
#
ifeq ($(shell uname -s),Haiku)
	CONFIG_LDFLAGS += $(LIB_NETWORK)
else
	CONFIG_LDFLAGS += $(LIB_C)
endif
CONFIG_LDFLAGS += $(LIB_MATH)

MAKE_OPTS=CC="$(CC)" -f Makefile.config --no-print-directory

comma = ,

#
# $1 = test program
# $2 = HAVE config name
# $3 = "using" message
# $4 = libraries required
# $5 = cflags required for test
# $6 = target clone settings
# $7 = cflags to be added to CONFIG_CFLAGS and test
# $8 = temp file
#
define check_tmp
	@if [ ! -f $(CONFIGS)/$(2) ]; then \
		$(CC) $(CFLAGS) $(CPPFLAGS) -Itest $(5) $(7) -DTARGET_CLONE=$(6) $(DIR)/$(1).c $(4) -o $(8) $(LDFLAGS) 2> /dev/null || true ; \
		if [ -s $(8) ]; then \
			echo "using $(3) ... yes"; \
			echo "# using $(2)" >> $(CONFIGS)/$(2); \
			if [ -n "$(7)" ]; then \
				echo "CONFIG_CFLAGS += $(7)" >> $(CONFIGS)/$(2); \
			fi; \
			if [ -n "$(4)" ]; then \
				for I in $(4); do \
					echo "CONFIG_LDFLAGS += $$I" >> $(CONFIGS)/$(2); \
				done; \
			fi; \
		else \
			echo "using $(3) ... no"; \
		fi; \
		mkdir -p $(CONFIGS); \
		touch $(CONFIGS)/$(2); \
		rm -f $(8); \
	fi
endef

define check
	$(call check_tmp,$1,$2,$3,$4,$5,$6,$7,$(shell mktemp))
endef

#
# $1 = test program
# $2 = HAVE config name
# $3 = "using" message
# $4 = libraries required
# $5 = cflags required for test
# $6 = target clone settings
# $7 = cflags to be added to CONFIG_CFLAGS and test
# $8 = temp file
#
define check_tmp_gxx
	@if [ ! -f $(CONFIGS)/$(2) ]; then \
		if [ -n "$(USING_GCC)" -a -n "$(USING_GXX)" ]; then \
			echo "building eigen matrix test..."; \
			$(CXX) $(CFLAGS) $(CPPFLAGS) -Itest $(5) $(7) -DTARGET_CLONE=$(6) $(DIR)/$(1).cpp $(4) -o $(8) $(LDFLAGS) 2> /dev/null || true ; \
			if [ -s $(8) ]; then \
				echo "using $(3) ... yes"; \
				echo "# using $(2)" >> $(CONFIGS)/$(2); \
				if [ -n "$(7)" ]; then \
					echo "CONFIG_CFLAGS += $(7)" >> $(CONFIGS)/$(2); \
				fi; \
				if [ -n "$(4)" ]; then \
					for I in $(4); do \
						echo "CONFIG_LDFLAGS += $$I" >> $(CONFIGS)/$(2); \
					done; \
				fi; \
			else \
				echo "using $(3) ... no"; \
			fi; \
			mkdir -p $(CONFIGS); \
			touch $(CONFIGS)/$(2); \
			rm -f $(8); \
		else \
			echo "using $(3) ... no, not using gcc"; \
		fi; \
	fi
endef

#
#  check for g++ compilation
#
define check_gxx
	$(call check_tmp_gxx,$1,$2,$3,$4,$5,$6,$7,$(shell mktemp))
endef

#
# $1 = float type
# $2 = HAVE config name
# $3 = message
# $4 = temp file
#
define check_float_tmp
	@if [ ! -f $(CONFIGS)/$(2) ]; then \
		$(CC) $(CFLAGS) $(CPPFLAGS) -DFLOAT=$(1) -o $(4) $(DIR)/test-float.c $(LIB_M) 2> /dev/null || true; \
		if [ -s $(4) ]; then \
			echo "using $(3) ... yes"; \
			echo "" >> $(CONFIGS)/$(2); \
		else \
			echo "using $(3) ... no"; \
		fi; \
	fi
	@mkdir -p $(CONFIGS)
	@touch $(CONFIGS)/$2
	@rm -f $4
endef

define check_float
	$(call check_float_tmp,$1,$2,$3,$(shell mktemp))
endef

#
# $1 = test program
# $2 = HAVE config name
# $3 = message
# $4 = temp file
#
define check_vecmath_tmp
	@if [ ! -f $(CONFIGS)/$(2) ]; then \
		$(CC) $(CFLAGS) $(CPPFLAGS) -Itest -DHAVE_VECMATH -c -o $(4) $(1).c 2> /dev/null || true; \
		if [ -s $(4) ]; then \
			echo "using $(3) ... yes"; \
			echo "" >> $(CONFIGS)/$(2); \
		else \
			echo "using $(3) ... no"; \
		fi; \
	fi
	@mkdir -p $(CONFIGS)
	@touch $(CONFIGS)/$(2)
	@rm -f $(4)
endef

define check_vecmath
	$(call check_vecmath_tmp,$1,$2,$3,$(shell mktemp))
endef

#
# $1 = test program
# $2 = HAVE config name
# $3 = message
# $4 = libs
#
define check_apparmor
	@if [ ! -f $(CONFIGS)/$(2) ]; then \
		$(CC) $(CFLAGS) $(CPPFLAGS) $(DIR)/$(1).c $4 -o $(DIR)/$(1) 2> /dev/null || true; \
		if [ -f $(DIR)/$(1) ]; then \
			$(APPARMOR_PARSER) -Q usr.bin.pulseaudio.eg -o apparmor-data.bin 2> /dev/null; \
			if [ $$? -eq 0 ] && [ -x "$(APPARMOR_PARSER)" ] && [ -f apparmor-data.bin ]; then \
				echo "using $(3) ... yes"; \
				echo "# using $(2)" >> $(CONFIGS)/$(2); \
				echo "CONFIG_LDFLAGS += $(4)" >> $(CONFIGS)/$(2); \
			else \
				echo "using $(3) ... no"; \
			fi; \
		else \
			echo "using $(3) ... no"; \
		fi; \
		mkdir -p $(CONFIGS); \
		touch $(CONFIGS)/$(2); \
	fi
	@rm -f $(DIR)/test-apparmor apparmor-data.bin
endef

#
# $1 = header file
# $2 = HAVE config name
# $3 = temp directory
#
define check_header_tmp
	@if [ ! -f $(CONFIGS)/$(2) ]; then \
		echo "#include <stdio.h>" >> $(3)/a.c; \
		echo "#include <sys/types.h>" >> $(3)/a.c; \
		for I in $1; do \
			echo "#include <$$I>" >> $(3)/a.c; \
		done; \
		$(CC) $(CFLAGS) $(CPPFLAGS) -c -o $(3)/a.o $(3)/a.c 2> /dev/null || true; \
		if [ -f $(3)/a.o ]; then \
			echo "using $(1) ... yes"; \
			echo "" >> $(CONFIGS)/$(2); \
		else \
			echo "using $(1) ... no"; \
		fi; \
		rm -rf $(3); \
		mkdir -p $(CONFIGS); \
		touch $(CONFIGS)/$(2); \
	fi
endef

define check_header
	$(call check_header_tmp,$1,$2,$(shell mktemp -d))
endef

#
# $1 = test program
# $2 = HAVE config name
# $3 = linker
#
define check_ld_tmp
	@if [ ! -f $(CONFIGS)/$(2) ]; then \
		$(CC) -fuse-ld=$(3) $(CFLAGS) $(CPPFLAGS) $(DIR)/$(1).c -o $(1) 2> /dev/null || true; \
		if [ -f $(1) ]; then \
			echo "using -fuse-ld=$(3) ... yes"; \
			echo "CONFIG_LDFLAGS += -fuse-ld=$(3)" >> $(CONFIGS)/$(2); \
		else \
			echo "using -fuse-ld=$(3) ... no"; \
		fi; \
		rm -f $(1); \
		mkdir -p $(CONFIGS); \
		touch $(CONFIGS)/$(2); \
	fi
endef

define check_ld
	$(call check_ld_tmp,$1,$2,$3)
endef

#
# Note echo "" > config.sort is required to workaround a makefile issue
# on Nixos
#
all: libraries headers cpufeatures types symbols functions
	@for I in $$(ls $(CONFIGS)); do \
		if [ -s $(CONFIGS)/$$I ]; then \
			echo "#define $$I"; \
		else \
			echo "/* #define $$I */"; \
		fi; \
	done | sort | uniq > config.h
	@echo $$(wc -l < config.h) configurations checked
	@rm -f $(CONFIGS)/HAVE_LDFLAGS_TMP
	@for I in $(CONFIG_LDFLAGS); do \
		echo "CONFIG_LDFLAGS += $$I" >> $(CONFIGS)/HAVE_LDFLAGS_TMP; \
	done
	@cat $(CONFIGS)/* | grep -v "#" | grep CONFIG_LDFLAGS | sed '/^$$/d' | sort | uniq > config.sort
	@rm -f config
	@for I in $(LIB_ORDER); do \
		grep "CONFIG_LDFLAGS += $$I\$$" config.sort >> config; \
		echo "" >> config.sort; \
	done
	@rm -f config.sort $(CONFIGS)/HAVE_LDFLAGS_TMP

configdir:
	@mkdir -p $(CONFIGS)

