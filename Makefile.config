#
#  check if we can build atomic related code
#
.PHONY: have_atomic
have_atomic: test/test-atomic_clear.c
	@$(CC) $(CFLAGS) $(CPPFLAGS) test/test-atomic_clear.c $(LIB_ATOMIC) -o stress-atomic_clear 2> /dev/null || true
	@if [ -f stress-atomic_clear ]; then \
		(./stress-atomic_clear > /dev/null 2>&1) && echo 1 || echo 0 ; \
	else \
		echo 0 ; \
	fi
	@rm -rf stress-atomic_clear
